{
  "name": "ember-cli-webcomponents-bundler",
  "version": "1.2.0",
  "description": "Allows to use Web Components using ES modules in your Ember app",
  "keywords": [
    "ember-addon",
    "web components",
    "ES6 modules",
    "bundle"
  ],
  "repository": "https://github.com/BBVAEngineering/ember-cli-webcomponents-bundler.git",
  "license": "MIT",
  "author": "BBVAEngineering",
  "directories": {
    "test": "tests"
  },
  "files": [
    "index.js",
    "lib/**"
  ],
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --fix --max-warnings 0 ./*.js config tests lib",
    "start": "ember serve",
    "pretest": "npm run clean-coverage",
    "test": "npm-run-all test:*",
    "test:ember": "COVERAGE=true ember test",
    "test:node": "nyc mocha --recursive 'node-tests/**/*-test.js'",
    "clean-coverage": "rm -rf coverage",
    "merge-coverage": "cp coverage/ember/coverage-final.json coverage/ember.json && cp coverage/node/coverage-final.json coverage/node.json && nyc merge coverage coverage/coverage-final.json",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "posttest": "npm run merge-coverage && npm run report-coverage && npm run check-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "autoprefixer": "^9.7.6",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^2.3.1",
    "broccoli-rollup": "^4.1.1",
    "deep-extend": "^0.6.0",
    "ember-cli-babel": "^7.19.0",
    "postcss": "^7.0.27",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-terser": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@ember/optional-features": "^0.7.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "chai": "^4.2.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "ember-ajax": "^5.0.0",
    "ember-cli": "^3.17.0",
    "ember-cli-code-coverage": "^1.0.0-beta.9",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^5.3.0",
    "ember-source": "^3.17.3",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-bbva": "^3.2.2",
    "eslint-plugin-ember": "^6.10.1",
    "eslint-plugin-node": "^10.0.0",
    "fs-extra": "^8.0.1",
    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "lit-element": "^2.3.1",
    "loader.js": "^4.7.0",
    "mocha": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "qunit-dom": "^0.9.2",
    "semantic-release": "^17.2.3",
    "sinon": "^8.1.1",
    "weightless": "0.0.34",
    "yeoman-assert": "^3.1.1"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://bbvaengineering.github.io/ember-cli-webcomponents-bundler"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
